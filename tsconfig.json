{
    "include": ["src/**/*.ts", "src/scrollPage.js", "utils/isNewListings.ts", "utils/validateListings.ts", "utils/groupListingData.ts"],
    "compilerOptions": {
        "outDir": "./dist",
        "paths": {
            "@/*": ["./*"]
        },
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "target": "es2018",
        "module": "nodenext",
        "allowSyntheticDefaultImports": true
    },
    "exclude": ["node_modules"],
    "baseUrl": "./"
}